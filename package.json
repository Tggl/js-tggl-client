{
  "name": "tggl-client",
  "version": "2.1.2",
  "description": "Tggl Typescript SDK for client and server",
  "keywords": [
    "Tggl",
    "Feature flag",
    "Feature toggle",
    "A/B testing",
    "Remote config",
    "Kill switch"
  ],
  "author": "Nicolas Keller",
  "license": "ISC",
  "homepage": "https://tggl.io/developers/sdks/node",
  "repository": "https://github.com/Tggl/js-tggl-client",
  "main": "lib/index.js",
  "browser": {
    "./lib/apiCall.js": "./lib/apiCallBrowser.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/.",
    "test": "jest",
    "prepublishOnly": "npm test && rm -rf lib && npm run build",
    "prepare": "husky install",
    "preversion": "npm test",
    "version": "node scripts/syncVersion.mjs && npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.10",
    "husky": "^6.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0",
    "ts-jest": "^29.1.4",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "dataloader": "^2.1.0",
    "tggl-core": "^1.3.0"
  },
  "files": [
    "lib/**/*"
  ]
}
